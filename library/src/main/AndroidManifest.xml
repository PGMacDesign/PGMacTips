<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.pgmacdesign.pgmacutilities">

    <application>
        <provider
            android:authorities="${applicationId}.pgmaccontextprovider"
            android:exported="false"
            android:enabled="true"
            android:name=".misc.PGMacInitProvider" />


        <!-- Gson -->
        <uses-library
            android:name="com.google.code.gson"
            android:required="false"
            />
        <!-- Retrofit 2 -->
        <uses-library
            android:name="com.squareup.retrofit2"
            android:required="false"
            />
        <!-- OkHttp -->
        <uses-library
            android:name="com.squareup.okhttp3"
            android:required="false"
            />
        <!-- Secure Prefs -->
        <uses-library
            android:name="com.securepreferences"
            android:required="false"
            />
        <!-- ElasticDownload -->
        <uses-library
            android:name="is.arontibo.library"
            android:required="false"
            />
        <!-- UCrop -->
        <uses-library
            android:name="com.yalantis.ucrop"
            android:required="false"
            />
        <!-- Animated SVG View -->
        <uses-library
            android:name="com.jaredrummler.android.animatedsvgview"
            android:required="false"
            />
        <!-- Volley -->
        <uses-library
            android:name="com.android.volley"
            android:required="false"
            />
        <!-- Picasso -->
        <uses-library
            android:name="com.squareup.picasso"
            android:required="false"
            />
        <!-- Nine Old Androids -->
        <uses-library
            android:name="com.nineoldandroids"
            android:required="false"
            />
        <!-- Okio -->
        <uses-library
            android:name="com.squareup.okio"
            android:required="false"
            />
        <!-- Android Animations -->
        <uses-library
            android:name="com.daimajia.androidanimations.library"
            android:required="false"
            />
        <!-- Part of Android Animations -->
        <uses-library
            android:name="com.daimajia.easing"
            android:required="false"
            />

    </application>
